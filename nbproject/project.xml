<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>jmstoolkit-logging</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <libraries xmlns="http://www.netbeans.org/ns/ant-project-libraries/1">
            <definitions>../nblibs/nblibraries.properties</definitions>
        </libraries>
        <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
            <config-files>
                <config-file>src/com/jmstoolkit/logging/console/mdb-context.xml</config-file>
                <config-file>src/infrastructure-context.xml</config-file>
                <config-file>src/jmx-context.xml</config-file>
                <config-file>src/logging-context.xml</config-file>
            </config-files>
            <config-file-groups/>
        </spring-data>
        <swingapp xmlns="http://www.netbeans.org/ns/form-swingapp/1">
            <application-class name="com.jmstoolkit.logging.console.JTKLogViewerApp"/>
        </swingapp>
    </configuration>
</project>
